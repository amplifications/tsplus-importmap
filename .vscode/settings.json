{
  "autoHide.autoHideSideBar": false,
  "better-comments.highlightPlainText": false,
  "better-comments.multilineComments": true,
  "better-comments.tags": [
    {
      "backgroundColor": "transparent",
      "bold": false,
      "color": "#FF2D00",
      "italic": false,
      "strikethrough": false,
      "tag": "!",
      "underline": false
    },
    {
      "backgroundColor": "transparent",
      "bold": false,
      "color": "#3498DB",
      "italic": false,
      "strikethrough": false,
      "tag": "?",
      "underline": false
    },
    {
      "backgroundColor": "#transparent",
      "bold": false,
      "color": "#98C379",
      "italic": false,
      "strikethrough": false,
      "tag": "*",
      "underline": false
    },
    {
      "backgroundColor": "transparent",
      "bold": false,
      "color": "#666666",
      "italic": false,
      "strikethrough": false,
      "tag": "۰",
      "underline": false
    },
    {
      "backgroundColor": "transparent",
      "bold": false,
      "color": "#666666",
      "italic": false,
      "strikethrough": false,
      "tag": "#region",
      "underline": false
    },
    {
      "backgroundColor": "transparent",
      "bold": false,
      "color": "#666666",
      "italic": false,
      "strikethrough": false,
      "tag": "#endregion",
      "underline": false
    },
    {
      "backgroundColor": "transparent",
      "bold": false,
      "color": "#AAAAAA",
      "italic": false,
      "strikethrough": false,
      "tag": "###",
      "underline": false
    },
    {
      "backgroundColor": "transparent",
      "bold": false,
      "color": "#BBBBBB",
      "italic": false,
      "strikethrough": false,
      "tag": "##",
      "underline": false
    },
    {
      "backgroundColor": "transparent",
      "bold": true,
      "color": "#FFFFFF",
      "italic": false,
      "strikethrough": false,
      "tag": "#",
      "underline": false
    }
  ],
  "editor.fontFamily": "\"Fira Code\",Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.hover.enabled": true,
  "editor.inlayHints.fontSize": 10,
  "editor.inlineSuggest.enabled": true,
  "editor.showFoldingControls": "always",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "comment.block.documentation",
        "settings": {
          "foreground": "#999999"
        }
      }
    ]
  },
  "editor.unicodeHighlight.allowedCharacters": {
    "ʼ": true,
    "۰": true,
    "Ⳇ": true
  },
  "editor.unicodeHighlight.ambiguousCharacters": true,
  "eslint.validate": ["typescript", "typescriptreact"],
  "explorer.autoReveal": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.excludeGitIgnore": true,
  "explorer.openEditors.sortOrder": "alphabetical",
  "explorer.sortOrder": "mixed",
  "explorer.sortOrderLexicographicOptions": "default",
  "files.associations": {
    "*.log": "vt100"
  },
  "foam.edit.linkReferenceDefinitions": "withoutExtensions",
  "foam.graph.titleMaxLength": 48,
  "foam.openDailyNote.directory": "",
  "foam.openDailyNote.titleFormat": "",
  "fold.level": 3,
  "git.decorations.enabled": true,
  "git.enableSmartCommit": true,
  "git.ignoreSubmodules": true,
  "highlight.regexes": {
    "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:FIXME|FIX|BUG|UGLY|DEBUG|HACK)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
      "decorations": [
        {
          "backgroundColor": "#cc0000",
          "color": "#1f1f1f",
          "fontWeight": "bold",
          "overviewRulerColor": "#cc0000"
        },
        {
          "backgroundColor": "#cc0000",
          "color": "#1f1f1f"
        }
      ],
      "filterFileRegex": ".*(?<!CHANGELOG.md)$"
    },
    "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:IDEA)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
      "decorations": [
        {
          "backgroundColor": "#cc00cc",
          "color": "#1f1f1f",
          "fontWeight": "bold",
          "overviewRulerColor": "#cc00cc"
        },
        {
          "backgroundColor": "#cc00cc",
          "color": "#1f1f1f"
        }
      ],
      "filterFileRegex": ".*(?<!CHANGELOG.md)$"
    },
    "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:REVIEW|OPTIMIZE|TSC)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
      "decorations": [
        {
          "backgroundColor": "#00ccff",
          "color": "#1f1f1f",
          "fontWeight": "bold",
          "overviewRulerColor": "#00ccff"
        },
        {
          "backgroundColor": "#00ccff",
          "color": "#1f1f1f"
        }
      ],
      "filterFileRegex": ".*(?<!CHANGELOG.md)$"
    },
    "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *TODO(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
      "decorations": [
        {
          "backgroundColor": "#ffcc00",
          "color": "#1f1f1f",
          "fontWeight": "bold",
          "overviewRulerColor": "#ffcc00"
        },
        {
          "backgroundColor": "#ffcc00",
          "color": "#1f1f1f"
        }
      ],
      "filterFileRegex": ".*(?<!CHANGELOG.md)$"
    }
  },
  "jest.autoRun": {
    "onSave": "test-file",
    "onStartup": ["all-tests"],
    "watch": true
  },
  "jest.coverageFormatter": "GutterFormatter",
  "jest.jestCommandLine": "npm run test --",
  "jest.showCoverageOnLoad": true,
  "jumpSource.testFileExtension": "sameAsSource",
  "jumpSource.testFileSuffix": "tspec",
  "jumpSource.testSubFolder": "",
  "LineCount.excludes": [
    "**/*.png",
    "**/.vscode/**",
    "**/node_modules/**",
    "**/out/**",
    "**/test.coverage/**",
    ".dependencygraph/**",
    "package*.json"
  ],
  "LineCount.order": "desc",
  "LineCount.output": {
    "csv": false,
    "json": false,
    "md": true,
    "outdir": "./out-saved/",
    "txt": false
  },
  "LineCount.sort": "totalline",
  "LineCount.statistics": true,
  "liveServer.settings.port": 5505,
  "npm.exclude": ["**/out/**"],
  "pasteImage.basePath": "${currentFileDir}/_.res.resources",
  "pasteImage.filePathConfirmInputBoxMode": "onlyName",
  "pasteImage.insertPattern": "${imageSyntaxPrefix}_.res.resources/${imageFilePath}${imageSyntaxSuffix}",
  "pasteImage.path": "${currentFileDir}/_.res.resources",
  "pasteImage.showFilePathConfirmInputBox": true,
  "peacock.color": "#42b883",
  "prettier.enableDebugLogs": true,
  "prettier.singleQuote": true,
  "regionHighlighterDimmer.defaultColor": "lightblack",
  "regionHighlighterDimmer.defaultTheme": "Default Color",
  "regionHighlighterDimmer.opacity": "0.75",
  "scm.countBadge": "focused",
  "scm.diffDecorationsGutterAction": "none",
  "scm.diffDecorationsGutterPattern": {
    "added": true
  },
  "scm.diffDecorationsGutterVisibility": "always",
  "scm.diffDecorationsGutterWidth": 5,
  "scm.providerCountBadge": "auto",
  "terminal.integrated.fontFamily": "MesloLGS NF",
